<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
                android:dither="true">
            <stroke
                android:color="#FF999999"
                android:width="2dp"/>
            <padding
                android:top="0dp"
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#00000000"/>
            <stroke
                android:color="#FF999999"
                android:width="2dp"/>
            <corners
                android:radius="13dp"/>
        </shape>
    </item>
</layer-list>